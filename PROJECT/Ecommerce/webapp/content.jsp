<!-- Product Section Begin -->
<section class="product spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 col-md-4">
                <div class="section-title">
                    <h4>New product</h4>
                </div>
            </div>
            <div class="col-lg-8 col-md-8">
                <ul class="filter__controls">
                    <li class="active" data-filter="*">All</li>
                    <li data-filter=".women">Women’s</li>
                    <li data-filter=".men">Men’s</li>
                    <li data-filter=".kid">Kid’s</li>
                    <li data-filter=".accessories">Accessories</li>
                    <li data-filter=".cosmetic">Cosmetics</li>
                </ul>
            </div>
        </div>
        <div class="row property__gallery">
            
             <%
                                    
                                    	Connection cn=new DBUtil().getConnectionData();
                                    PreparedStatement st=cn.prepareStatement("select * from addproduct");
                                    ResultSet rs=st.executeQuery();
                                    while(rs.next())
                                    {
                                    %>
            <div class="col-lg-3 col-md-4 col-sm-6 mix women men kid accessories cosmetic">
           
                <div class="product__item">
                
                    <div class="product__item__pic set-bg" data-setbg="productpic/<%=rs.getString(5) %>">
                        <!-- <div class="label new">New</div> -->
                        <ul class="product__hover">
                            <li><a href="productpic/<%=rs.getString(5) %>" class="image-popup"><span class="arrow_expand"></span></a></li>
                            <li><a href="#"><span class="icon_heart_alt"></span></a></li>
                            <li><a href="#"><span class="icon_bag_alt"></span></a></li>
                        </ul>
                    </div>
                    <div class="product__item__text">
                        <h6><a href="viewproduct.jsp?pid=<%= rs.getInt(1) %>"><%= rs.getString(2) %></a></h6>
                        <div class="rating">
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                            <i class="fa fa-star"></i>
                        </div>
                        <div class="product__price">Rs. <%= rs.getInt(4) %></div>
                    </div>
                    
                </div>
               
            </div>
            
             <% 
                                    }        
                	cn.close();
                           
                 %>
            </div>
        </div>
    
</section>
